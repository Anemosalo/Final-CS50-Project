

{
  "manifest_version": 3,
  "name": "Convolution Extension",
  "version": "0.01",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contents.js"]
    }
  ],
  "action": {
    
    "default_popup": "popup.html",
    "default_title": "here is the suposed extenstion"

  },
  "icons": {
    "16": "images/ct16.png",
    "32": "images/ct32.png",
    "48": "images/ct48.png",
    "128": "images/ct128.png"
  },
  "default_icon": {
      "16": "images/ct16.png",
      "32": "images/ct32.png",
      "48": "images/ct48.png",
      "128": "images/ct128.png"
   }
}